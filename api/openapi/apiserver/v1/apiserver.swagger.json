  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/healthz": {
      "get": {
        "summary": "服务健康检查",
        "operationId": "Healthz",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1HealthzResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "服务治理"
        ]
      }
    }
  },
  "definitions": {
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1HealthzResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/v1ServiceStatus",
          "title": "status 表示服务的健康状态"
        },
        "timestamp": {
          "type": "string",
          "title": "timestamp 表示请求的时间戳"
        },
        "message": {
          "type": "string",
          "title": "message 表示可选的状态消息，描述服务健康的更多信息"
        }
      },
      "title": "HealthzResponse 表示健康检查的响应结构体"
    },
    "v1ServiceStatus": {
      "type": "string",
      "enum": [
        "HEALTHY",
        "UNHEALTHY"
      ],
      "default": "HEALTHY",
      "description": "- HEALTHY: Healthy 表示服务健康\n - UNHEALTHY: Unhealthy 表示服务不健康",
      "title": "ServiceStatus 表示服务的健康状态"
    }
  }
}
